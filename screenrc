hardstatus alwayslastline "%{= KW} Dallas: %-w%{= wk} %50>%n %t%{-}%+w%< %=%{KW}[%c %M %d %Y ]"
startup_message off 
msgwait 2
defscrollback 30000
activity "activity in %n (%t) [%w:%s]~"
defutf8 on
defnonblock on
autodetach on

